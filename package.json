{
  "name": "tinkercad",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder --win zip"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "sharp": "^0.33.5"
  },
  "build": {
    "appId": "com.tinkercad.desktop",
    "productName": "Tinkercad Desktop",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "portable",
        "zip"
      ],
      "defaultArch": "x64",
      "icon": "build/autodesk-tinkercad.png"
    },
    "extraResources": [
      {
        "from": "arduino",
        "to": "../arduino",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "sketch",
        "to": "../sketch",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "electron-prompt": "^1.7.0",
    "winreg": "^1.2.5"
  }
}
